# Fantasy Basketball Service Environment Variables
# Copy this file to .env and fill in your values

# ESPN API Credentials
# Get these from: https://fantasy.espn.com
ESPN_S2=your_espn_s2_cookie_here
SWID=your_swid_here
LEAGUE_ID=your_league_id_here

# Flask Configuration (optional, defaults are provided)
FLASK_HOST=0.0.0.0
FLASK_PORT=5000
FLASK_ENV=production
FLASK_DEBUG=False

# Python Configuration
PYTHONUNBUFFERED=1
